<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:state_pressed="true" >
        <shape>
            <solid
                android:color="#f8f9fa" />
            <stroke
                android:width="1dp"
                android:color="#1d4d79" />
            <corners
                android:radius="3dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#62a4e1"
                android:endColor="#334f8f"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#1d4d79" />
            <corners
                android:radius="1dp" />

        </shape>
    </item>
</selector>